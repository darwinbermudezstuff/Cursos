<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    @Html.Partial("_Menu")
    <div class="container body-content">
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)

        @*@Scripts.Render("~/Content/dataTablesCSS")
        @Scripts.Render("~/bundles/dataTablesJS")*@


        <link href="//cdn.datatables.net/1.10.7/css/jquery.dataTables.css" rel="stylesheet" />
        <script src="//cdn.datatables.net/1.10.7/js/jquery.dataTables.js"></script>

        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Darwin Bermudez (<a href="mailto:darwin.bermudez@hotmail.com">darwin.bermudez@hotmail.com</a>)</p>
        </footer>
    </div>
    @Html.Partial("_ModalAction")


</body>
</html>
